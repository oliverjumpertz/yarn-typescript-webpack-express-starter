{
  "name": "yarn-typescript-webpack-express-starter",
  "version": "1.0.0",
  "private": true,
  "main": "dist/bundle.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn clear && tslint -c tslint.json -p tsconfig.json --fix",
    "build": "webpack",
    "postbuild": "yarn test",
    "prestart": "yarn build",
    "start": "node .",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "dev": "tsc",
    "postdev": "node dist/index.js",
    "clear": "rm -r dist/* || true"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/express": "^4.17.2",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.2",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.7.0",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
